{
  "Root Endpoint (GET /)": {
    "curl": "curl \"https://your-api-url.rapidapi.com/\" \\\n-H \"X-RapidAPI-Key: YOUR_API_KEY\" \\\n-H \"X-RapidAPI-Host: your-api-host.rapidapi.com",
    "expected_status": 200,
    "assertions": [
      {
        "type": "equals",
        "field": "status",
        "expected": "ok"
      },
      {
        "type": "equals",
        "field": "service",
        "expected": "ECA Marine API"
      },
      {
        "type": "exists",
        "field": "version"
      }
    ]
  },
  "Ping Endpoint (GET /ping)": {
    "curl": "curl \"https://your-api-url.rapidapi.com/ping\" \\\n-H \"X-RapidAPI-Key: YOUR_API_KEY\" \\\n-H \"X-RapidAPI-Host: your-api-host.rapidapi.com",
    "expected_status": 200,
    "assertions": [
      {
        "type": "equals",
        "field": "status",
        "expected": "ok"
      }
    ]
  },
  "Calculate Route Rotterdam To New York (POST /calculate_route)": {
    "curl": "curl -X POST \"https://your-api-url.rapidapi.com/calculate_route\" \\\n-H \"Content-Type: application/json\" \\\n-H \"X-RapidAPI-Key: YOUR_API_KEY\" \\\n-H \"X-RapidAPI-Host: your-api-host.rapidapi.com\" \\\n-d '{\"origin\": [51.9244, 4.4777], \"destination\": [40.7128, -74.006], \"restrictions\": [], \"include_explanation\": false}'",
    "expected_status": 200,
    "assertions": [
      {
        "type": "equals",
        "field": "origin",
        "expected": "ROTTERDAM"
      },
      {
        "type": "equals",
        "field": "destination",
        "expected": "NEW_YORK"
      },
      {
        "type": "equals",
        "field": "restrictions",
        "expected": []
      },
      {
        "type": "exists",
        "field": "distance_nm"
      },
      {
        "type": "exists",
        "field": "eca_distance_nm"
      },
      {
        "type": "exists",
        "field": "waypoints"
      },
      {
        "type": "range",
        "field": "distance_nm",
        "min": 3000,
        "max": 5000
      }
    ]
  },
  "Calculate Route Singapore To Rotterdam Via Suez (POST /calculate_route)": {
    "curl": "curl -X POST \"https://your-api-url.rapidapi.com/calculate_route\" \\\n-H \"Content-Type: application/json\" \\\n-H \"X-RapidAPI-Key: YOUR_API_KEY\" \\\n-H \"X-RapidAPI-Host: your-api-host.rapidapi.com\" \\\n-d '{\"origin\": [1.3521, 103.8198], \"destination\": [51.9244, 4.4777], \"restrictions\": [], \"include_explanation\": false}'",
    "expected_status": 200,
    "assertions": [
      {
        "type": "equals",
        "field": "traversed_passage",
        "expected": "suez"
      }
    ]
  },
